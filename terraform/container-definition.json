[
    {
        "name": "my-golang-app",
        "image": "<YOUR_IMAGE_URL>",
        "cpu": 256,
        "memoryReservation": 512,
        "essential": true,
        "portMappings": [
            {
                "containerPort": 8080,
                "protocol": "tcp"
            }
        ],
        "environment": [
            {
                "name": "VAR_NAME",
                "value": "VAR_VALUE"
            }
        ],
        "secrets": [
            {
                "name": "SECRET_NAME",
                "valueFrom": "arn:aws:secretsmanager:<AWS_REGION>:<AWS_ACCOUNT_ID>:secret:<SECRET_NAME>:<SECRET_KEY>"
            }
        ],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "<CLOUDWATCH_LOG_GROUP>",
                "awslogs-region": "<AWS_REGION>",
                "awslogs-stream-prefix": "<CLOUDWATCH_LOG_STREAM_PREFIX>"
            }
        }
    }
]